  n6 [label="print(f'{num2} > {num1}')\l"];
  n4 -> n6;
  n4 -> n6 [label="False"];
  n7 [label="\l", shape=ellipse];
  n5 -> n7;
  n6 -> n7;
  n8 [label="FIN\l", shape=ellipse];
  n7 -> n8;
