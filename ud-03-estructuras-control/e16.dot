digraph G {
  rankdir=TB;
  node [fontname="Arial", fontsize=12];

  start [shape=ellipse, style=filled, fillcolor=lightgreen, label="Inicio"];
  end [shape=ellipse, style=filled, fillcolor=lightcoral, label="Fin"];

  n0 [shape=box, style=filled, fillcolor="lightskyblue", label="num = -1"];
  n1 [shape=diamond, style=filled, fillcolor="gold", label="while num < 0 or num > 99999:"];
  n2 [shape=box, style=filled, fillcolor="lightskyblue", label="num = str(num)"];
  n3 [shape=parallelogram, style=filled, fillcolor="mediumpurple1", label="print(f\"El número tiene {len(num)} dígitos\")"];

  subgraph cluster_true_n1 {
    style=filled; color=lightgrey; label="Verdadero";
    t2_1 [shape=box, style=filled, fillcolor="white", label="num = int(input(\"Introduzca un número de 0 a 99999: \"))"];
    t2_3 [shape=parallelogram, style=filled, fillcolor="mediumpurple1", label="print('Error. El número ha de ser entre 0 a 99999')"];
    t2_1 -> t2_3;
  }

  subgraph cluster_false_n1 {
    style=filled; color=lightblue; label="Falso";
  }

  start -> n0;
  n0 -> n1;
  n1 -> cluster_true_n1 [lhead=cluster_true_n1];
  n1 -> cluster_false_n1 [lhead=cluster_false_n1];
  n1 -> n2;
  n2 -> n3;
  n3 -> end;
}